module(
  unparameterized("Keywords")
, [imports([module(unparameterized("languages/sdf2/syntax/Sdf2"))])]
, [ exports(
      context-free-syntax(
        [ prod(
            [sort("Constructor")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("cons"), [fun(quoted("\"Constructor\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"recover\"")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(fun(unquoted(\\\"recover\\\"))))\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"completion\"")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(fun(unquoted(\\\"completion\\\"))))\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"incremental\"")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(fun(unquoted(\\\"incremental\\\"))))\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"deprecated\"")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(fun(unquoted(\\\"deprecated\\\"))))\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"deprecated\""), lit("\"(\""), sort("StrCon"), lit("\")\"")]
