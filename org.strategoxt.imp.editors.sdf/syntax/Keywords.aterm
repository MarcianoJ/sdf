          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(appl(unquoted(\\\"deprecated\\\"), [fun(quoted(<1>))])))\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"ast\""), lit("\"(\""), sort("StrCon"), lit("\")\"")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(appl(unquoted(\\\"ast\\\"), [fun(quoted(<1>))])))\""))]))
                )
              ]
            )
          )
        , prod(
            [lit("\"cons\""), lit("\"(\""), sort("Constructor"), lit("\")\"")]
          , sort("Attribute")
          , attrs(
              [ prefer()
              , term(
                  default(appl(unquoted("ast"), [fun(quoted("\"term(default(appl(unquoted(\\\"cons\\\"), [fun(quoted(<1>))])))\""))]))
                )
              ]
            )
          )
        , prod([sort("StrCon")], sort("Constructor"), no-attrs())
