    "completion123"       -> IdCon {completion}

  context-free syntax
                          -> COMPLETIONPREFIX{ast("COMPLETION(\"\")")}

  context-free syntax
    "module" COMPLETIONPREFIX list:ImpSection* Sections -> Module {cons("Module")}    %%{ast("module(\"\", <1>)")}

