module SDF

imports
  languages/sdf2/syntax/Sdf2
  Naming
  Keywords
  
exports
  
  context-free start-symbols
    SDF Module

  %% HACK
  context-free syntax
    Group "." -> Group {non-assoc,cons("non-transitive")}
    
  %% Completion Rules
  lexical syntax
    
                          -> Sort {completion}
                          -> ModuleWord {completion}
                          -> IdCon {completion}

    "completion123"       -> Sort {completion}
