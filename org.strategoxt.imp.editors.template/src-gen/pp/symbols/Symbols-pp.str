module src-gen/pp/symbols/Symbols-pp

imports
  libstratego-gpp
  runtime/tmpl/pp
  src-gen/signatures/symbols/Symbols-sig


imports
  src-gen/signatures/layout/Comments-sig


imports
  src-gen/pp/layout/Comments-pp


strategies
  prettyprint-example =
    prettyprint-Symbol

  prettyprint-example =
    prettyprint-Symbols

  prettyprint-example =
    prettyprint-SymbolDef

  prettyprint-Symbol :
    Parenthetical(t0__) -> [ H(
                               [SOpt(HS(), "0")]
                             , [ S("(")
                               , t0__'
                               , S(")")
                               ]
                             )
                           ]
    with t0__' := <pp-one-Z(prettyprint-Symbol)> t0__

  prettyprint-Symbols :
    list__ -> [ H(
                  [SOpt(HS(), "0")]
                , [list__']
                )
              ]
    where not(is-Symbols)
    where list__' := <pp-H-list(prettyprint-Symbol)> list__

  prettyprint-SymbolDef :
    t0__ -> [ H(
                [SOpt(HS(), "0")]
              , [t0__']
              )
            ]
    where not(is-SymbolDef)
    where t0__' := <pp-one-Z(prettyprint-Symbol)> t0__

  is-Symbol =
    fail

  is-Symbols =
    fail

  is-SymbolDef =
    fail