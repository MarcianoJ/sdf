module MiniJava

imports
 
  Lexical	
  Expressions	
  Statements	
  Classes

context-free start-symbols

  Start Program ClassDecl Exp FieldDecl MainClass MethodDecl Statement Type VarDecl ParamDecl ID INT

sorts 

  Program MainClass

context-free syntax

  Start = Program 
  Program.Program = <
    <MainClass>

    <{ClassDecl "\n\n"}*>
  > 
  MainClass.MainClass = <
    class <ID> { 
      public static void main (String[] <ID>) { 
      	<Statement> 
    	}
    }> 

template options

  keyword -/- [A-Za-z]
  tokenize : "[()"
