//Grammar6

module ExprInj

context-free syntax

	Start      = Stm
	Stm.If     = <if <BExp> then <Stm>>
	Stm.Assign = <<Var> = <Exp>>
	BExp.Lt    = [[Num] < [Num]]
	BExp.True  = <true>
	Exp        = BExp
	Exp.Add    = <<Exp> + <Num>>
	Exp        = Num

lexical syntax
	
	Num = "0" | "1"
	Var = "a" | "b"