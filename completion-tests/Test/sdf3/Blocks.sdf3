//Grammar9

module Blocks

imports SampleGrammars

context-free start-symbols
    Start

context-free syntax

	Start = Class
	Class.Class = <
				c <Name> {
					<Meths>
				}>
	Meths.Some = Meth Meths
	Meths.None = 
	Meth.Method = <
				m <Name>{
					<Stms>
				}>
	Stms.Some = Stm Stms
	Stms.None =
	Stm.Print = <p <Num>> 
	Stm.Read  = <r <Var>>
	
lexical syntax
	
	Num = "0" | "1"
	Var = "a" | "b"
	Name = "A" | "B"
	
//context-free priorities
 //   Meths.Some > Meths.None	