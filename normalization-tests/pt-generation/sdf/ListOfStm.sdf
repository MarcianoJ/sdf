module ListOfStm
exports
  context-free start-symbols
    Start

  context-free syntax
    Stms     -> Start 
    Stm Stms -> Stms  {cons("Some")}
             -> Stms  {cons("None")}
    "p" Num  -> Stm   {cons("Print")}
    "r" Var  -> Stm   {cons("Read")}

  lexical syntax
    "0" | "1" -> Num 
    "a" | "b" -> Var 