module Optional
exports
  context-free start-symbols
    Start

  context-free syntax
    Mod            -> Start 
    "m" Sects      -> Mod   {cons("Module")}
    Sect Sects     -> Sects {cons("Some")}
                   -> Sects {cons("None")}
    "s" Opt Rules  -> Sect  {cons("Section")}
    "opt:" Opts    -> Opt   {cons("Opt")}
                   -> Opt   {cons("NoOpt")}
    Rule Rules     -> Rules {cons("Some")}
                   -> Rules {cons("None")}
    NonT "=" NonTs -> Rule  {cons("Rule")}
    NonT NonTs     -> NonTs {cons("Some")}
                   -> NonTs {cons("None")}

  lexical syntax
    "A" | "B" -> NonT 
    "c" | "d" -> Opts 